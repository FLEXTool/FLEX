# platform :ios, '7.0'

target 'FLEX' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FLEX

  project 'FLEX'
  workspace 'FLEX.xcworkspace'

  target 'FLEXTestsMethodsList' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'UICatalog' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for UICatalog
  pod 'FLEX', :path => '.'

  project 'Example/UICatalog'

end
